    "    abecedary = list(alphabet)\n",
    "    print(s, abecedary)\n",
    "    for _ in abecedary:\n",
    "        if _ not in s:\n",
    "            print('{_} not in {s}')\n",
    "            return False\n",
    "        else:\n",
    "            return True"
